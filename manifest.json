{
  "manifest_version": 2,

  "name": "Flash Hub",
  "description": "Hub providing popular sentiment on the latest news.",
  "version": "1.0",

  "dependencies": {
    "cheerio": "latest",
    "request": "latest"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}